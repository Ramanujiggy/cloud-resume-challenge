{
  "version": "2.0",
  "extensions":{
    "cosmosDB": {
      "connectionMode": "Gateway"
      }
    },
  "logging":  {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "console":{
      "isEnabled":true,
      "DisableColors":true
    }
    
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  }
}